<div class="row">
  <div class="col-md-4">
    <h1 class="text-center">Send Email</h1>
    <%= form_for @customer do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <%= f.submit "Send!!!" %>
    <% end %>
  </div>
  <div class="col-md-8">
    <h1 class="text-center">Dashboard</h1>
  </div>
</div>
